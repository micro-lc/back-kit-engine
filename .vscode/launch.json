{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "jest: current test file",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["jest", "--runInBand", "${relativeFile}"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    }
  ]
}
